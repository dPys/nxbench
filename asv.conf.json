{
    "version": 1,
    "project": "nxbench",
    "timeout": 3000,
    "project_url": "https://github.com/dpys/nxbench",
    "repo": ".",
    "branches": [
        "main"
    ],
    "environment_type": "existing",
    "show_commit_url": "https://github.com/dpys/nxbench/commit/",
    "pythons": [
        "3.11"
    ],
    "matrix": {
        "req": {
            "networkx": [
                "3.4.2"
            ],
            "nx_parallel": [
                "0.3"
            ],
            "python-graphblas": [
                "2024.2.0"
            ]
        },
        "env_nobuild": {
            "NUM_THREAD": [
                "1",
                "4",
                "8"
            ],
            "OMP_NUM_THREADS": [
                "1"
            ],
            "MKL_NUM_THREADS": [
                "1"
            ],
            "OPENBLAS_NUM_THREADS": [
                "1"
            ]
        }
    },
    "benchmark_dir": "nxbench/benchmarks",
    "env_dir": "env",
    "results_dir": "results",
    "html_dir": "html",
    "hash_length": 8,
    "plugins": [
        "asv_runner"
    ],
    "build_command": [
        "PIP_NO_BUILD_ISOLATION=false python -m pip install ."
    ],
    "install_command": [
        "python -m pip install {wheel_file}"
    ],
    "uninstall_command": [
        "return-code=any python -m pip uninstall -y {project}"
    ]
}
